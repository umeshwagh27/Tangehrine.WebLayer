@model IEnumerable<Tangehrine.DataLayer.Model.Admin.ToDoList>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="cstm-offcanvas">
    <div class="card cstm-card">
        <div class="card-body" id="nav">
            <div class="canvas-header"><h2>To Do List</h2></div>
            <div class="canvas-body">
                <label for="" class="form-label">Enter Detail </label>
                <div class="mb-3">
                    <input type="text" class="form-control" id="message" placeholder="type here...." />
                </div>
                <div class="mb-3" id="data">
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>